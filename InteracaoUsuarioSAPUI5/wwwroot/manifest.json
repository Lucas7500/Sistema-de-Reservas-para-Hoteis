{
    "_version": "1.58.0",
    "sap.app": {
        "id": "reservas.hoteis",
        "i18n": "i18n/i18n.properties",
        "title": "{{tituloApp}}",
        "description": "{{decricaoApp}}",
        "type": "application",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "dependencies": {
            "minUI5Version": "1.108.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.webc.main": {},
                "sap.ui.table": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "reservas.hoteis.i18n.i18n",
                    "supportedLocales": [
                        "pt_BR",
                        "en"
                    ],
                    "fallbackLocale": ""
                }
            }
        },
        "rootView": {
            "viewName": "reservas.hoteis.view.App",
            "type": "XML",
            "id": "app"
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "type": "View",
                "viewType": "XML",
                "path": "reservas.hoteis.view",
                "controlId": "app",
                "controlAggregation": "pages"
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "listagem",
                    "target": "listagem"
                },
                {
                    "pattern": "cadastro",
                    "name": "adicionar",
                    "target": "adicionar"
                },
                {
                    "pattern": "detalhes/{id}",
                    "name": "detalhes",
                    "target": "detalhes"
                }
            ],
            "targets": {
                "listagem": {
                    "id": "listagem",
                    "name": "Listagem"
                },
                "adicionar": {
                    "id": "adicionar",
                    "name": "Adicionar"
                },
                "detalhes": {
                    "id": "detalhes",
                    "name": "Detalhes"
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    }
}